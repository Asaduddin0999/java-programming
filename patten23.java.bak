

class patten23
{
	public static void main(String[] args) 
	{
		int space =4;
			int star =1;
		for (int i=1;i<=5 ;i++)
		{
			for (int a=1;a<=space ;a++)
				{
					System.out.print(" ");	
				}
				for (int j=1;j<=star ;j++ )
				{
				System.out.print("* ");	
				}
				space--;
				star=star+2;
				System.out.println( );
			}
			
		}
		
	}

