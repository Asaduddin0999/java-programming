
//Nth_term_fibonacci

class PRT_fibo01 
{
	public static void main(String[] args) 
	{
		int sln=1;
		int a= 0;
		int b= 1;
		int sum=0;
		for (int i=1;sum<=10;i++ )
		{
			//int sum=1;
			
			a=b;
			b=sum;
			sum=a+b;
			
		}
		for (int j=1;j<=10 ;j++ )
		{
			System.out.println(sln+")"+sum);
			
			sum=b;
			b=a
				a=sum-b;
			sln++;
		}
		 
	}
}
