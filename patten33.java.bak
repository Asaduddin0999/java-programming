class patten33
{
	public static void main (String[]args )
	{
		int i,j,o;
			for (i= 1;j<=5 ;j++ )
			{
			System.out.print(" ");
			}
			for (j=1;j<=i ;j++ )
			{
				System.out.print("*");
			}
			for (o=1;o<=1 ;o++ )
			{
				System.out.print(" ");
			}
			System.out.println();
	}
}