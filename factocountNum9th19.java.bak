//19 count the factors with 9 ?


import java.util.Scanner;
class factocountNum9th19

{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner (System.in);
		System.out.println("Enter first number ");
		int b = scan.nextInt();
		System.out.println("Enter of count number is");
		int count = 0;
		for ( int i=1;i<=b  ;i++ )
			{
			if(b% i ==0)
		{
			 count++;
			 System.out.println(i);
		}
		}
			System.out.println("--------------------------------");
			System.out.println(count);
			System.out.println("--------------------------------");

	}
}
