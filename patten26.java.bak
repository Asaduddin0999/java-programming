class  patten26
{
	public static void main(String[] args) 
	{
		int star =1;
		int space =4;
		for (int i=1;i<=9;i++ )
		{
			for (int j=1;j<=space;j++ )
			{
			System.out.print(" ");
			}
			for (b=1;b<=star ;b++ )
			{
				System.out.println("*");
			}
		      
				if (i<=4)
				{
					space--;
					star++;	
				}
				else 
				{
				space++;
				star--;	
				}
				 
		}
	}
			
			
}
		
	



 