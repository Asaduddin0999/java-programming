import java.util.Scanner;
class pritn1to5Patten 
{
	public static void main(String[] args) 
	{ 
		Scanner scan = new Scanner (System.in);
		System.out.print("Enter a how many row lift side trangle:-" );
		int num = scan.nextInt();
		for (int i=1;i<=num ;i++ )
		{
			for (int j=1;j<=i ;j++ )
			{
				System.out.print(j);
			}
				System.out.println();
		}
		
	}
}
