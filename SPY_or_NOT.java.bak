//45-wap to chick wather a given sum is SPY num or not ?

import java.util.Scanner;
class  SPY_or_NOT
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a num");
		int num=scan.nextInt();
		int sum=0;
		int product=1;
		while (num!=0)
		{
			int ld =num%10;
			sum=sum+ld;
			product=product*ld;
			num=num/10;
		}
		if (num=product)
		{
			System.out.println("this is SPY num");
		}
		else 
		{
		System.out.println("this is not SPY num ");
		}
	}
}
