	
class  patten32
{
	public static void main(String[] args) 
	{
		int star =5;
		int space =0;
		for (int i=1;i<=5;i++ )
		{
			for (int j=1;j<=star;j++ )
			{
			System.out.print("*");
			}
			for (int o=1;o<=space ;o++ )
			{
				System.out.print("*");
			}
			for (int t=1;t<=star ;t++ )
			{
				System.out.print("*");
			}
		       
				 
							
				
				System.out.println();
				star--;
					space+=2;
				 
		}
	}
			
			
}
		
	



 


 