

class patten24
{
	public static void main(String[] args) 
	{
		int space =0;
			int star =9;
		for (int i=1;i<=5 ;i++)
		{
			for (int a=1;a<=space ;a++)
				{
					System.out.print(" ");	
				}
				for (int j=1;j<=star  ;j-- )
				{
				System.out.print("*");	
				}
				star++;
				space=space+2;
				System.out.println( );
			}
			
		}
		
	}

