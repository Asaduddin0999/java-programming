class  patten29
{
	public static void main(String[] args) 
	{
		int star =1;
		int space =4;
		for (int i=1;i<=9;i++ )
		{
			/*for (int j=1;j<=star;j++ )
			{
			System.out.print("*");
			}*/
			for (int o=1;o<=space ;o++ )
			{
				System.out.print(" ");
			}
			for (int t=1;t<=star ;t++ )
			{
				System.out.print("*");
			}
		       
				if (i>=5)
				{
					star=star-2;
						space++;
				}
				else 
				{
					star=star+2;
						space--;
							
				}
				System.out.println();
				 
		}
	}
			
			
}
		
	



 